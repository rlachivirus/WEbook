<!DOCTYPE html>
<html>
  <head>
    <title>WEbook</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%#= favicon_link_tag asset_path('logo_only.png') %>

    <script type="text/javascript">
      window.linkedin = "<%= image_url('linkedinLogo.png') %>";
      window.github = "<%= image_url('githubLogo.png') %>";
      window.portfolio = "<%= image_url('portfolio.png') %>";
      window.thumbsUp = "<%= image_url('thumbsUp.png') %>";
      window.thumbsUpIcon = "<%= image_url('thumbsUpIcon.png') %>";
    </script>

    <%= favicon_link_tag asset_path('WE_favicon.png') %>

  </head>

  <body>
    <%= yield %>
  </body>
</html>
